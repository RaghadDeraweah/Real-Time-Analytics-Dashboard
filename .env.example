# Core services
NODE_ENV=development
LOG_LEVEL=info

# System 1 defaults
SYSTEM1_HTTP_PORT=4000
SYSTEM1_HTTP2_PORT=4001
SYSTEM1_WS_PORT=4002
SYSTEM1_WORKER_COUNT=4
SYSTEM1_REDIS_HOST=redis
SYSTEM1_REDIS_PORT=6379
SYSTEM1_REDIS_DB=0
SYSTEM1_REDIS_USERNAME=
SYSTEM1_REDIS_PASSWORD=

# System 2 defaults
SYSTEM2_WS_PORT=4100
SYSTEM2_AGG_WINDOW_MS=1000
SYSTEM2_BUFFER_SIZE=500

# Fake metric servers
FAKE_SERVER_COUNT=10
FAKE_SERVER_INTERVAL_MS=1000
FAKE_HTTP_TARGET=http://localhost:4000/metrics
FAKE_WS_TARGET=ws://localhost:4100

# Load test configuration
LOAD_TEST_TARGET_HTTP=http://localhost:4000/metrics
LOAD_TEST_TARGET_WS=ws://localhost:4100
LOAD_TEST_DURATION=60
